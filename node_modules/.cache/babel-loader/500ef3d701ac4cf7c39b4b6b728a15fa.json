    this.totalRecords = response.headers.get(\"X-Total-Count\");\n      } catch (e) {\n        console.error(e);\n      }\n    },\n\n    updateContentPageChanged(computedPage) {\n      this.page = computedPage;\n      this.loadProducts();\n    },\n\n    updateFilters(target) {\n      const affectedFilter = this.filters[target.name];\n\n      if (affectedFilter.type == \"radio\") {\n        affectedFilter.selected = [target.value];\n      } else {\n        if (!affectedFilter.selected.includes(target.value)) {\n          affectedFilter.selected.push(target.value);\n        } else {\n          affectedFilter.selected = affectedFilter.selected.filter(val => {\n            return val != target.value;\n          });\n        }\n      }\n\n      this.page = 1;\n      this.loadProducts();\n    }\n\n  },\n\n  mounted() {\n    this.loadProducts();\n  }\n\n};","map":{"version":3,"mappings":";AAyBA,OAAOA,WAAP,MAAwB,+BAAxB;AACA,OAAOC,aAAP,MAA0B,iCAA1B;AACA,OAAOC,aAAP,MAA0B,iCAA1B;AAEA,eAAe;EACbC,UAAU,EAAE;IACVD,aADU;IAEVF,WAFU;IAGVC;EAHU,CADC;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,YAAY,EAAE,CAFT;MAGLC,eAAe,EAAE,CAHZ;MAILC,IAAI,EAAE,CAJD;MAKLC,OAAO,EAAE;QACPC,GAAG,EAAE;UACHC,IAAI,EAAE,KADH;UAEHC,IAAI,EAAE,UAFH;UAGHC,OAAO,EAAE,SAHN;UAIHC,KAAK,EAAE,CAAC,OAAD,EAAU,SAAV,CAJJ;UAKHC,cAAc,EAAE,CAAC,OAAD,EAAU,SAAV,CALb;UAMHC,QAAQ,EAAE;QANP,CADE;QASPC,UAAU,EAAE;UACVN,IAAI,EAAE,WADI;UAEVC,IAAI,EAAE,UAFI;UAGVC,OAAO,EAAE,UAHC;UAIVC,KAAK,EAAE,CAAC,MAAD,EAAS,OAAT,EAAkB,QAAlB,CAJG;UAKVC,cAAc,EAAE,CAAC,MAAD,EAAS,OAAT,EAAkB,QAAlB,CALN;UAMVC,QAAQ,EAAE;QANA,CATL;QAiBPE,IAAI,EAAE;UACJP,IAAI,EAAE,YADF;UAEJC,IAAI,EAAE,OAFF;UAGJC,OAAO,EAAE,MAHL;UAIJC,KAAK,EAAE,CAAC,cAAD,EAAiB,eAAjB,EAAkC,eAAlC,CAJH;UAKJC,cAAc,EAAE,CAAC,OAAD,EAAU,OAAV,EAAmB,OAAnB,CALZ;UAMJC,QAAQ,EAAE,CAAC,cAAD,CANN;UAOJG,KAAK,EAAE,CAAC,MAAD;QAPH;MAjBC;IALJ,CAAP;EAiCD,CAxCY;;EAyCbC,OAAO,EAAE;IACP,MAAMC,YAAN,GAAqB;MACnB,IAAI;QACF,MAAMC,YAAW,GAAI,IAAIC,eAAJ,CAAoB;UACvCC,MAAM,EAAE,KAAKjB,eAD0B;UAEvCkB,KAAK,EAAE,KAAKjB;QAF2B,